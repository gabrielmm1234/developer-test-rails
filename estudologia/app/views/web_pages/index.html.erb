<p id="notice"><%= notice %></p>

<h1><%= t 'web_pages' %></h1>

<table>
  <thead>
    <tr>
      <th><%= t 'title' %></th>
      <th><%= t 'description' %></th>
      <th><%= t 'body' %></th>
      <th><%= t 'author' %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_pages.each do |web_page| %>
      <tr>
        <td><%= web_page.title %></td>
        <td><%= web_page.description %></td>
        <td><%= web_page.body %></td>
        <td><%= web_page.author %></td>
        <td><%= link_to t('show'), web_page %></td>
        <td><%= link_to t('edit'), edit_web_page_path(web_page) %></td>
        <td><%= link_to t('destroy'), web_page, method: :delete, data: { confirm: t('delete') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('new_web_page'), new_web_page_path %>
